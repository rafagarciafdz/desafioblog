<h1>Listing Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Comentarios</th>
      <th>Votes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <div id="posts">
      <%= render partial: "posts/post", collection: @posts %>
    </div>
  </tbody>
</table>

<br>

<div id="pagination">
  <%= paginate @posts %>
</div>

<%= link_to 'New Post', new_post_path %>